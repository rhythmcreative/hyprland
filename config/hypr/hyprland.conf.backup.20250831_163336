###################
### KEYBINDINGS ###
###################

# See https://wiki.hypr.land/Configuring/Keywords/
$mainMod = SUPER # Sets "Windows" key as main modifier

# Example binds, see https://wiki.hypr.land/Configuring/Binds/ for more
bind = $mainMod, Return, exec, kitty
bind = $mainMod SHIFT, Return, exec, warp-terminal
bind = $mainMod, Q, killactive,
bind = $mainMod, M, exit,
bind = $mainMod, E, exec, thunar
bind = $mainMod, W, togglefloating,
# Rofi adaptativo con style-3.rasi que se ajusta automÃ¡ticamente al monitor activo
bind = $mainMod, A, exec, ~/.local/bin/rofi-style3-monitor-adaptive
# Rofi run mode (comandos) - Super + R
bind = $mainMod, R, exec, ~/.local/bin/adaptive-rofi run
# Rofi window selector - Super + Tab
bind = $mainMod, Tab, exec, ~/.local/bin/adaptive-rofi window
# bind = $mainMod, P, pseudo, # dwindle - Movido para evitar conflicto con screenshot

# Screenshot with swappy - Super + P
bind = $mainMod, P, exec, grim -g "$(slurp)" - | swappy -f -
bind = $mainMod, J, togglesplit, # dwindle

# Toggle fullscreen with Alt + Enter
bind = ALT, Return, fullscreen, 0

# Lock screen with hyprlock
bind = $mainMod, L, exec, hyprlock

# Powermenu de rofi con detecciÃ³n automÃ¡tica de monitor
bind = $mainMod, BackSpace, exec, ~/.local/bin/powermenu-with-monitor-detection
bind = $mainMod, XF86Back, exec, ~/.local/bin/pywal-wallpaper-sync
bind = $mainMod, Prior, exec, ~/.local/bin/pywal-wallpaper-sync
bind = $mainMod SHIFT, BackSpace, exec, ~/.local/bin/powermenu-with-monitor-detection


# Wallpaper changer ADAPTATIVO con sincronizaciÃ³n AUTOMÃTICA de SDDM - Super + Shift + W
bind = $mainMod SHIFT, W, exec, ~/.local/bin/wallpaper-change-adaptive && sleep 1 && ~/.local/bin/sddm-auto-sync-local

# Wallpaper selector SIMPLE (evita problemas con workspaces) - Super + Alt + W  
bind = $mainMod ALT, W, exec, ~/.local/bin/wallpaper-changer-with-waybar-sync

# Recarga SEGURA de Waybar - Super + Alt + R
bind = $mainMod ALT, R, exec, ~/.local/bin/simple-waybar-sync

# Recarga manual de Waybar con sincronizaciÃ³n pywal - Super + Shift + R
bind = $mainMod SHIFT, R, exec, ~/.local/bin/reload-waybar-simple

# Reinicio de emergencia de Waybar - Super + Ctrl + R
bind = $mainMod CTRL, R, exec, ~/.local/bin/restart-waybar-simple

# Limpieza MEJORADA de waybar duplicados - Super + Ctrl + Shift + R
bind = $mainMod CTRL SHIFT, R, exec, ~/.local/bin/waybar-cleanup-improved

# SincronizaciÃ³n manual de nm-applet con pywal - Super + Shift + N (nueva versiÃ³n mejorada)
bind = $mainMod SHIFT, N, exec, ~/.local/bin/nm-applet-auto-sync --force

# Move focus with mainMod + arrow keys
bind = $mainMod, left, movefocus, l
bind = $mainMod, right, movefocus, r
bind = $mainMod, up, movefocus, u
bind = $mainMod, down, movefocus, d

# Switch workspaces with mainMod + [0-9]
bind = $mainMod, 1, workspace, 1
bind = $mainMod, 2, workspace, 2
bind = $mainMod, 3, workspace, 3
bind = $mainMod, 4, workspace, 4
bind = $mainMod, 5, workspace, 5
bind = $mainMod, 6, workspace, 6
bind = $mainMod, 7, workspace, 7
bind = $mainMod, 8, workspace, 8
bind = $mainMod, 9, workspace, 9
bind = $mainMod, 0, workspace, 10

# Move active window to a workspace with mainMod + SHIFT + [0-9]
bind = $mainMod SHIFT, 1, movetoworkspace, 1
bind = $mainMod SHIFT, 2, movetoworkspace, 2
bind = $mainMod SHIFT, 3, movetoworkspace, 3
bind = $mainMod SHIFT, 4, movetoworkspace, 4
bind = $mainMod SHIFT, 5, movetoworkspace, 5
bind = $mainMod SHIFT, 6, movetoworkspace, 6
bind = $mainMod SHIFT, 7, movetoworkspace, 7
bind = $mainMod SHIFT, 8, movetoworkspace, 8
bind = $mainMod SHIFT, 9, movetoworkspace, 9
bind = $mainMod SHIFT, 0, movetoworkspace, 10

# Screenshot with swappy
bind = $mainMod SHIFT, Print, exec, grim -g "$(slurp)" - | swappy -f -

# Example special workspace (scratchpad)
bind = $mainMod, S, togglespecialworkspace, magic
bind = $mainMod SHIFT, S, movetoworkspace, special:magic

# Scroll through existing workspaces with mainMod + scroll
bind = $mainMod, mouse_down, workspace, e+1
bind = $mainMod, mouse_up, workspace, e-1

# Move/resize windows with mainMod + LMB/RMB and dragging
bindm = $mainMod, mouse:272, movewindow
bindm = $mainMod, mouse:273, resizewindow

# Laptop multimedia keys for volume and LCD brightness - no notifications
bindel = ,XF86AudioRaiseVolume, exec, wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+
bindel = ,XF86AudioLowerVolume, exec, wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-
bindel = ,XF86AudioMute, exec, wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle
bindel = ,XF86AudioMicMute, exec, wpctl set-mute @DEFAULT_AUDIO_SOURCE@ toggle
bindel = ,XF86MonBrightnessUp, exec, brightnessctl -e4 -n2 set 5%+
bindel = ,XF86MonBrightnessDown, exec, brightnessctl -e4 -n2 set 5%-

# Requires playerctl
bindl = , XF86AudioNext, exec, playerctl next
bindl = , XF86AudioPause, exec, playerctl play-pause
bindl = , XF86AudioPlay, exec, playerctl play-pause
bindl = , XF86AudioPrev, exec, playerctl previous

# Toggle bluetooth with F10
bind = , F10, exec, ~/.local/bin/toggle-bluetooth

# Volume control with F11/F12
bind = , F11, exec, wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-
bind = , F12, exec, wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+

# Keyboard backlight control (Fn+F2/F3 on ASUS)
bind = , XF86KbdBrightnessDown, exec, ~/.local/bin/keyboard-backlight down
bind = , XF86KbdBrightnessUp, exec, ~/.local/bin/keyboard-backlight up


##############################
### WINDOWS AND WORKSPACES ###
##############################

# See https://wiki.hypr.land/Configuring/Window-Rules/ for more
# See https://wiki.hypr.land/Configuring/Workspace-Rules/ for workspace rules

# Example windowrule
# windowrule = float,class:^(kitty)$,title:^(kitty)$

# Ignore maximize requests from apps. You'll probably like this.
windowrule = suppressevent maximize, class:.*

# Fix some dragging issues with XWayland
windowrule = nofocus,class:^$,title:^$,xwayland:1,floating:1,fullscreen:0,pinned:0

# nwg-dock-hyprland window rules - superposiciÃ³n elegante sobre ventanas
windowrulev2 = float, class:^(nwg-dock-hyprland)$
windowrulev2 = pin, class:^(nwg-dock-hyprland)$
windowrulev2 = noborder, class:^(nwg-dock-hyprland)$
windowrulev2 = noshadow, class:^(nwg-dock-hyprland)$
windowrulev2 = noblur, class:^(nwg-dock-hyprland)$
windowrulev2 = stayfocused, class:^(nwg-dock-hyprland)$


